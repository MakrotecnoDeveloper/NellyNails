@{
    string nombre;
    var nombreClaim = User.FindFirst("Nombre");
    if (nombreClaim != null && nombreClaim.Value != null)
    {
        nombre = nombreClaim.Value;
    }
    else
    {
        nombre = "No se está sincronizando la información en la BD";
    }
}

